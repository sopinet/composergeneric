{
    "name": "sopinet/composergeneric",
    "version": "0.0.1",
    "description": "Generic composer",
    "keywords": ["composer"],
    "discard-changes" : "true",
    "type": "",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Luis Miguel Lopez",
            "email": "llopez@sopinet.com",
            "homepage": "http://sopinet.com"
        },
        {
            "name": "Fernando Hidalgo",
            "email": "fhidalgo@sopinet.com",
            "homepage": "http://sopinet.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.10.2",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.10.2.js",
                    "type": "file"
                }
            }
        }
    ],    
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.4.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/data-fixtures": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",
        "incenteev/composer-parameter-handler": "~2.0",
        "kriswallsmith/assetic": "1.2.*",
        "sonata-project/doctrine-extensions": "1.*",
        "sonata-project/easy-extends-bundle" : "2.1.*",
        "sonata-project/cache-bundle": "dev-master",
        "sonata-project/jquery-bundle": "1.8.*",
        "sonata-project/exporter": "1.1.0",
        "sonata-project/google-authenticator": "1.*",
        "sonata-project/user-bundle": "2.2.*",
        "sonata-project/block-bundle": "2.2.*",
        "sonata-project/core-bundle" : "~2.2@dev",
        "twitter/bootstrap": "2.3.*",
       	"craue/formflow-bundle": "dev-master",
       	"sonata-project/doctrine-orm-admin-bundle": "2.2.*",
	    "knplabs/knp-components": "1.2.1",
	    "knplabs/knp-paginator-bundle": "~2.3",
        "knplabs/knp-menu-bundle": "1.1.*",
        "stof/doctrine-extensions-bundle": "dev-master",
        "a2lix/translation-form-bundle" : "2.x-dev",
        "sopinet/bootstrap-extend-bundle": "dev-master",
        "friendsofsymfony/rest-bundle": "dev-master",
        "friendsofsymfony/user-bundle" : "1.3.*",
        "jms/serializer-bundle": "dev-master",
        "jms/security-extra-bundle": "dev-master",
        "jms/di-extra-bundle": "dev-master",
        "raulfraile/ladybug-bundle": "v0.7",
        "twbs/bootstrap": "3.0.*",
        "jquery/jquery":  "1.10.*",  
        "leafo/lessphp": "0.4.0",      
        "braincrafted/bootstrap-bundle": "~2.0"
    },
	"scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sopinet\\Bundle\\BootstrapExtendBundle\\Composer\\ScriptHandler::copyExport",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sopinet\\Bundle\\BootstrapExtendBundle\\Composer\\ScriptHandler::copyExport",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
             "dev-master": "2.3-dev"
        }
    }
}
